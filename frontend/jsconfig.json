{
    "compilerOptions": {
        "baseUrl": "./", // Base URL of the projectes
        "paths": {
          "@app/*": [
            "src/*" // Adjusted for general usage in the src folder
          ],
          "@dashboard/*": [
            "src/features/dashboard/*" 
          ],
          "@auth/*": [
            "src/features/auth/*" 
          ],
          "@landing/*": [
            "src/features/landing/*" 
          ],
          "@errorPage/*": [
            "src/features/errorPage/*" 
          ],
          "@uploads/*": [
            "src/features/uploads/*" 
          ],
          "@layouts/*": [
            "src/features/layouts/*" 
          ],
          "@components/*": [
            "src/features/components/*" 
          ],
          "@public/*": [
            "public/*"
          ],
          "@assets/*": [
            "src/assets/*"
          ],
          "@utils/*": [
            "src/app/utils/*" 
          ],
          "@hooks/*": [
            "src/app/hooks/*" 
          ],
          "@configs/*": [
            "src/app/configs/*" 
          ],
          "@enums/*": [
            "src/app/enums/*" 
          ],
          "@props/*": [
            "src/app/props/*" 
          ],
          "@interfaces/*": [
            "src/app/interfaces/*" 
          ],
          "@types/*/": [
            "src/app/types/*" 
          ],
          "@api/*": [
            "src/app/api/*" 
          ],
          "@store/*": [
            "src/app/store/*" 
          ]
        }
      },   
}